  <property name="plugins" type="lines">
    <element value="ajaxsave;/++resource++cke_ajaxsave/plugin.js"/>
  </property>
  <property name="remove_plugins" type="lines">
  </property>
  <property name="customTemplates" type="lines">
  </property>
  <property name="templatesReplaceContent" type="boolean">False</property>
  <property name="bodyId" type="string">content</property>
  <property name="bodyClass" type="string"></property>

