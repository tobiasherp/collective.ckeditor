  { name : 'Inserted Text',        element : 'ins' },
  { name : 'Cited Work',           element : 'cite' },
  { name : 'Inline Quotation',     element : 'q' },
  { name : 'Language: RTL',        element : 'span', attributes : { 'dir' : 'rtl' } },
  { name : 'Language: LTR',        element : 'span', attributes : { 'dir' : 'ltr' } },
